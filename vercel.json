  {
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "routes": [
      {
  
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "functions": {
        "api/users/**/*.js": {
        "maxDuration": 30
        }
    }
  }